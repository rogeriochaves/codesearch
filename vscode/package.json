{
  "name": "codesearch",
  "displayName": "Codesearch",
  "description": "Quickly search for code snippets from VS Code, no need to switch to the browser and disrupt your flow",
  "publisher": "rogeriochaves",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogeriochaves/codesearch.git"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "icon.png",
  "extensionKind": [
    "ui"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codesearch.openSearchBox",
        "title": "Open Codesearch"
      }
    ],
    "keybindings": [
      {
        "command": "codesearch.openSearchBox",
        "key": "alt+space",
        "mac": "alt+space"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "copy-server": "cp -R ../codesearch/ python-server && cp ../requirements.txt python-server/",
    "pip-install": "pip install -r python-server/requirements.txt --target=python-server/",
    "compile": "npm run copy-server && webpack",
    "watch": "npm run copy-server && webpack --watch",
    "package": "npm run copy-server && npm run pip-install && webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.10",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "clipboardy": "^2.3.0",
    "node-fetch": "^2.6.1"
  }
}
